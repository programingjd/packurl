{
  "$schema": "https://json.schemastore.org/web-manifest-combined.json",
  "name": "Packurl",
  "short_name": "Packurl",
  "start_url": ".",
  "display": "standalone",
  "background_color": "#282828",
  "theme_color": "#4564ba",
  "description": "Pack some data into a web https address (URL) and share it. Visiting that URL automatically unpacks it. Your data is never sent to any server to protect your privacy.",
  "icons": [
    {
      "src": "https://cdn.packurl.net/favicon.svg",
      "sizes": "any",
      "type": "image/svg+xml",
      "purpose": "any"
    },
    {
      "src": "https://cdn.packurl.net/mask.svg",
      "sizes": "any",
      "type": "image/svg+xml",
      "purpose": "maskable"
    },
    {
      "src": "https://cdn.packurl.net/favicon.png",
      "sizes": "1024x1024",
      "type": "image/png",
      "purpose": "any"
    }
  ]
}
